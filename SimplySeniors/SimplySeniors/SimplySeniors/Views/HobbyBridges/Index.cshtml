@model IEnumerable<SimplySeniors.Models.HobbyBridge>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Hobby.NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProfileID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Hobby.NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProfileID)
        </td>
    </tr>
}

</table>
